{
  "name": "vike-trpc-ssr",
  "description": "Use TRPC with Vike",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "dotenv -c development -- npm run server:dev",
    "lint": "eslint . --max-warnings 0",
    "prettier": "prettier --write .",
    "prod": "npm run lint && npm run build && npm run server:prod",
    "server": "node --experimental-specifier-resolution=node --loader ./server/loader.js ./server/index.ts",
    "server:dev": "npm run server",
    "server:prod": "cross-env NODE_ENV=production npm run server"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.44.1",
    "@trpc/react-query": "^10.44.1",
    "@trpc/server": "^10.44.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.17",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vite-plugin-vercel/vike": "^2.0.0",
    "@vitejs/plugin-basic-ssl": "^1.0.2",
    "@vitejs/plugin-react": "^4.2.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.54.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "prettier": "^3.1.0",
    "prettier-plugin-packagejson": "^2.4.6",
    "sirv": "^2.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2",
    "vike": "^0.4.147",
    "vite": "^5.0.2",
    "vite-plugin-vercel": "^2.0.0"
  }
}
